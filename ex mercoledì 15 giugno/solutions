-- GROUP BY

1)
SELECT COUNT(`id`) AS `n_iscritti`, YEAR(`enrolment_date`) AS `anno_iscrizione`
FROM `students`
GROUP BY `anno_iscrizione`;

2)
SELECT COUNT(`id`) AS `insegnanti`, `teachers`.`office_address` AS `indirizzo`
FROM `teachers`
GROUP BY `indirizzo`;

3)
SELECT ROUND(AVG(`vote`)) AS `media_voti`, `exam_id` AS `appello_esame`
FROM `exam_student`
GROUP BY `appello_esame`;

4)
SELECT COUNT(`id`) AS `n_corsi_laurea`, `department_id` AS `dipartimenti`
FROM `degrees`
GROUP BY `dipartimenti`;